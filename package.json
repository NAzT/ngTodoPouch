{
  "name": "todo-pouch",
  "version": "0.0.0",
  "description": "An AngularJS-Atomify-PouchDb Tutorial",
  "scripts": {
    "watch": "atomify -d",
    "start": "node server.js",
    "test": "tap app/**/*_test.js",
    "dev": "npm run watch & npm start"
  },
  "repository": "",
  "author": "",
  "readmeFilename": "readme.md",
  "devDependencies": {
    "tap": "^0.4.9",
    "handlebars": "^1.3.0",
    "hbsfy": "^1.3.2",
    "atomify": "^1.5.0-beta7",
    "nodemon": "^1.0.17",
    "supertest": "^0.12.0",
    "pouchdb": "^2.2.0"
  },
  "dependencies": {
    "express": "^4.1.1",
    "nano": "^5.8.0",
    "underscore": "^1.6.0",
    "body-parser": "^1.0.2",
    "async": "^0.8.0",
    "request": "^2.34.0",
    "morgan": "^1.0.1"
  },
  "atomify": {
    "js": {
      "entry": "app/index.js",
      "output": "www/bundle.js",
      "watch": true
    },
    "css": {
      "entry": "app/index.css",
      "output": "www/bundle.css"
    }
  },
  "engines": {
    "node": "0.10.x",
    "npm": "1.4.x"
  }
}
