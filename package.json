{
  "name": "todolists",
  "version": "0.1.5-1",
  "description": "An AngularJS-Atomify-PouchDb Tutorial",
  "scripts": {
    "build": "atomify && uglifyjs ./www/bundle.js -o ./www/bundle.min.js",
    "start": "node server.js",
    "test": "tap app/**/*_test.js",
    "dev": "npm run watch & npm start"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/twilson63/ngTodoPouch.git"
  },
  "author": "Tom",
  "devDependencies": {
    "tap": "^0.4.9",
    "handlebars": "^1.3.0",
    "hbsfy": "^1.3.2",
    "atomify": "^1.5.0-beta7",
    "nodemon": "^1.0.17",
    "supertest": "^0.12.0",
    "uglify-js": "^2.4.13"
  },
  "dependencies": {
    "express": "^4.1.1",
    "nano": "^5.8.0",
    "underscore": "^1.6.0",
    "body-parser": "^1.0.2",
    "async": "^0.8.0",
    "request": "^2.34.0",
    "morgan": "^1.0.1"
  },
  "atomify": {
    "js": {
      "entry": "app/index.js",
      "output": "www/bundle.js"
    },
    "css": {
      "entry": "app/index.css",
      "output": "www/bundle.css"
    }
  },
  "engines": {
    "node": "0.10.x",
    "npm": "1.4.x"
  },
  "subdomain": "todolists",
  "main": "server.js",
  "keywords": [
    "Ultimate",
    "Todo",
    "App"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/twilson63/ngTodoPouch/issues"
  },
  "homepage": "https://github.com/twilson63/ngTodoPouch"
}
